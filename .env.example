# Well Monitor Environment Variables
# Copy this file to .env and update with your actual values

# Secrets Management Mode
WELLMONITOR_SECRETS_MODE=environment

# Azure IoT Hub Connection String
WELLMONITOR_IOTHUB_CONNECTION_STRING=HostName=YourIoTHub.azure-devices.net;DeviceId=YourDevice;SharedAccessKey=YourKey

# Azure Storage Connection String
WELLMONITOR_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=yourstorageaccount;AccountKey=YourKey;EndpointSuffix=core.windows.net

# Local Encryption Key (for local data encryption)
WELLMONITOR_LOCAL_ENCRYPTION_KEY=your-32-character-encryption-key-here

# PowerApp API Key
WELLMONITOR_POWERAPP_API_KEY=your-powerapp-api-key

# OCR API Key (Azure Cognitive Services or other)
WELLMONITOR_OCR_API_KEY=your-ocr-api-key

# Azure Key Vault Settings (optional - for Key Vault mode)
# KeyVault__Uri=https://your-keyvault.vault.azure.net/
# AZURE_CLIENT_ID=your-managed-identity-client-id
# AZURE_TENANT_ID=your-tenant-id
